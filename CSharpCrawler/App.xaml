<Application x:Class="CSharpCrawler.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException" Startup="Application_Startup" >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Colors.xaml"/>
                <ResourceDictionary Source="Style/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Style/WindowStyle.xaml"/>
                <ResourceDictionary Source="Style/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/CheckBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/TabControlStyle.xaml"/>
                <ResourceDictionary Source="Style/ScrollViewerStyle.xaml"/>
                <ResourceDictionary Source="Style/ListViewStyle.xaml"/>
                <ResourceDictionary Source="Style/ToolTipStyle.xaml"/>
                <ResourceDictionary Source="Style/GroupBoxStyle.xaml"/>
                <ResourceDictionary Source="Style/SliderStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
